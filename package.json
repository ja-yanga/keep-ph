  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --turbo",
    "start": "next start",
    "lint": "eslint . --ext .ts --ext .tsx",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:integration": "jest __tests__/integration --runInBand --no-coverage",
    "test:single": "jest --runInBand --no-coverage --verbose",
    "test:lighthouse:dashboard": "jest __tests__/integration/lighthouse/admin-dashboard-lighthouse.test.tsx --runInBand",
    "test:lighthouse:dashboard:view": "node -e \"const fs=require('fs');const path=require('path');const reportsDir=path.join('__tests__','integration','lighthouse','reports');const files=fs.existsSync(reportsDir)?fs.readdirSync(reportsDir).filter(f=>f.endsWith('.report.html')).sort().reverse():[];if(files.length>0){const latest=path.resolve(reportsDir,files[0]);console.log('Opening:',latest);require('child_process').exec(`start ${latest.replace(/\\//g,'\\\\')}`);}else{console.log('No reports found in',reportsDir);}\"",
    "test:staged": "jest --findRelatedTests",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
